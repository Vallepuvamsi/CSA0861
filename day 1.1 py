def vamsi(x,y):
    if len(x)!=len(y):
        return False
    for i in range(len(x)):
        count=0
        if x.count(x[i])!=y.count(y[i]):
            return False
    return True
print(vamsi("fool","poor"))
print(vamsi("foal","poor"))
print(vamsi("too","bar"))
print(vamsi("toto","yaya"))
